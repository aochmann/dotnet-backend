{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build WebApi",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/WebApi/WebApi.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Build End2End Tests",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/test/EndToEndTests/EndToEndTest.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Build Integration Tests",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/test/IntegrationTests/IntegrationTest.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Build Unit Tests",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/test/UnitTests/UnitTest.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}